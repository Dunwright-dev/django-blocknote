{
  "name": "django-blocknote-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite build --mode development --logLevel warn",
    "watch": "NODE_ENV=development vite build --watch --mode development",
    "serve": "vite --port 5174",
    "prod": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "clean": "rm -rf ../django_blocknote/static/django_blocknote/dist/* && rm -rf ../django_blocknote/static/django_blocknote/dist/.vite"
  },
  "dependencies": {
    "@blocknote/core": "^0.41.1",
    "@blocknote/mantine": "^0.41.1",
    "@blocknote/react": "^0.41.1",
    "@mantine/core": "^8.3.4",
    "@mantine/hooks": "^8.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  }
}
